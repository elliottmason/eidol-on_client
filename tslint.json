{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-prettier",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint:all"
  ],
  "jsRules": {},
  "rules": {
    "invalid-void": [
      true,
      {
        "allow-generics": [
          "Promise"
        ]
      }
    ],
    "completed-docs": false,
    "file-name-casing": {
      "options": [
        "camel-case",
        "pascal-case"
      ]
    },
    "no-import-side-effect": [
      true,
      {
        "ignore-pattern": "/rxjs.*/"
      }
    ],
    "no-inferrable-types": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}
